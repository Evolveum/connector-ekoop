               <source>
                  <c:path>familyName</c:path>
               </source>
            </outbound>
         </attribute>
         <attribute>
            <c:ref>ri:telephoneNumber</c:ref>
            <outbound>
               <strength>strong</strength>
               <source>
                  <c:path>telephoneNumber</c:path>
               </source>
            </outbound>
         </attribute>
         <attribute>
            <c:ref>ri:cepTel2</c:ref>
